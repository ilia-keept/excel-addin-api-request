      "parameters": [
        {
          "name": "service",
          "description": "Music service name (e.g. \"deezer\")",
          "type": "string"
        },
        {
          "name": "query",
          "description": "Album search query (e.g. \"Queen News of the World\")",
          "type": "string"
        }
      ],
      "result": {
        "type": "string",
        "dimensionality": "matrix"
      }
    },
    {
      "id": "TRACK",
      "name": "TRACK",
      "description": "Gets detailed track information from a music service",
      "parameters": [
        {
          "name": "service",
          "description": "Music service name (e.g. \"deezer\")",
          "type": "string"
        },
        {
          "name": "query",
          "description": "Track query (e.g. \"Queen - We Will Rock You\")",
