{
  "allowCustomDataForDataTypeAny": true,
  "functions": [
    {
      "id": "ISRC",
      "name": "ISRC",
      "description": "Gets track info by ISRC code from a music service",
      "parameters": [
        {
          "name": "service",
          "description": "Music service name (e.g. \"deezer\")",
          "type": "string"
        },
        {
          "name": "isrc",
          "description": "ISRC code",
          "type": "string"
        }
      ],
      "result": {
        "type": "string"
      }
    },
    {
      "id": "ALBUM",
      "name": "ALBUM",
